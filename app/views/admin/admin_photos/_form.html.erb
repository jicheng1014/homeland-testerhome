<%= form_for([:admin,@photo], html: { enctype: "multipart/form-data" }) do |f| %>
  <fieldset>
    <legend></legend>
    <%= render 'shared/error_messages', target: @photo %>

    <div class="clearfix">
      <%= f.label :image %>
      <div class="form-group">
        <%= f.file_field :image, class: "form-control" %>
      </div>
    </div>
    <div class="form-actions mt-2">
      <%= f.submit t("common.upload"), class: "btn btn-primary", 'data-disable-with' => t("common.uploading") %>
      or
      <%= link_to 'Back', admin_photos_path %>
    </div>
<% end %>
